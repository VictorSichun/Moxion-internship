{
  "name": "salvatore",
  "version": "1.0.0",
  "description": "Salvatore",
  "repository": "https://github.com/tinderboxhq/Salvatore",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --inline --color --progress --history-api-fallback",
    "build": "webpack --color --progress",
    "watch": "webpack --color --progress --watch",
    "test": "xo"
  },
  "dependencies": {
    "angular": "^1.6.3",
    "angular-animate": "^1.6.3",
    "angular-aria": "^1.6.3",
    "angular-sanitize": "^1.6.3",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "^0.4.2",
    "fuse.js": "^3.0.5",
    "leaflet": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "ngstorage": "^0.3.11"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-angularjs-annotate": "^0.7.0",
    "babel-preset-es2015": "^6.24.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^4.5.1",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-notifier": "^1.5.0",
    "xo": "^0.18.1"
  },
  "xo": {
    "space": 4,
    "semicolon": false,
    "envs": [
      "browser"
    ],
    "rules": {
      "capitalized-comments": 0,
      "import/no-unassigned-import": 0,
      "max-params": 0
    },
    "globals": [
      "angular",
      "ionic"
    ]
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "angularjs-annotate"
    ]
  }
}
